# Environment
NODE_ENV=development
PORT=5000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/telemed_db

# Security & Auth
JWT_SECRET=your-jwt-secret-key-must-be-at-least-32-chars-long
JWT_ACCESS_SECRET=your-jwt-access-secret-key-32-chars-min
JWT_REFRESH_SECRET=your-jwt-refresh-secret-key-32-chars-min
SESSION_SECRET=your-session-secret-key-min-32-chars-long
COOKIE_SECRET=your-cookie-secret-key-min-16-chars

# AI Features (disabled by default in production)
AI_ENABLED=false
AI_SYMPTOMS_ENABLED=false
AI_ICD_SUGGESTION_ENABLED=false
AI_DRUG_INTERACTIONS_ENABLED=false

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL_PRIMARY=gpt-4
OPENAI_MODEL_FALLBACK=gpt-3.5-turbo
OPENAI_BACKOFF_MAX_RETRIES=3

# CORS Configuration
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Webhook & Alerts
ALERT_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL_ID=C1234567890

# Medical Data Encryption
MEDICAL_ENCRYPTION_KEY=your-medical-encryption-key-32-chars-min

# External Services
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
TWILIO_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_WHATSAPP=whatsapp:+14155238886

# Payment Processing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key

# Monitoring & Analytics
STATUSCAKE_API_KEY=your-statuscake-api-key
UPTIMEROBOT_API_KEY=your-uptimerobot-api-key
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-telegram-chat-id

# Replit Environment (auto-populated in Replit)
REPL_ID=your-repl-id
REPLIT_DOMAINS=your-repl-domain.replit.dev

# OAuth & Identity
ISSUER_URL=https://auth.replit.com