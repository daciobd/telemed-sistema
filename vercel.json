{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/start.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "node prepare-deployment.js && npm run build"
}